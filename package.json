  "browser": {
    "./src/shaders/index.js": "./src/shaders/shaders.js",
    "./src/util/window.js": "./src/util/browser/window.js",
    "./src/util/web_worker.js": "./src/util/browser/web_worker.js"
  },
  "esm": true,
  "scripts": {
    "build-dev": "rollup -c --environment BUILD:dev",
    "watch-dev": "rollup -c --environment BUILD:dev --watch",
    "build-prod": "rollup -c --environment BUILD:production",
    "build-directions": "NODE_ENV=production mkdir -p src/mapbox-gl-directions/dist && browserify -s MapboxDirections src/mapbox-gl-directions/src/index.js > src/mapbox-gl-directions/dist/mapbox-gl-directions.js && cp src/mapbox-gl-directions/src/mapbox-gl-directions.css src/mapbox-gl-directions/dist",
    "build-prod-min": "rollup -c --environment BUILD:production,MINIFY:true",
    "wemap-font-awesome": "cp node_modules/font-awesome/css/font-awesome.min.css dist/css && cp -r node_modules/font-awesome/fonts dist",
    "wemap-pre-build": "yarn run build-directions && yarn run wemap-tran-directions",
    "wemap-pre-build-geo": "yarn run build-directions && yarn run wemap-tran-directions && yarn run wemap-directions-add-option && yarn run wemap-directions-geocode",
    "wemap-tran-directions-trafic": "sed -i \"s/<label for='mapbox-directions-profile-driving-traffic'>Traffic/<label for='mapbox-directions-profile-driving-traffic'>Giao thông/g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-tran-directions-driving": "sed -i \"s/<label for='mapbox-directions-profile-driving'>Driving/<label for='mapbox-directions-profile-driving'>Ô tô/g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-tran-directions-walking": "sed -i \"s/<label for='mapbox-directions-profile-walking'>Walking/<label for='mapbox-directions-profile-walking'>Đi bộ/g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-tran-directions-cycling": "sed -i \"s/<label for='mapbox-directions-profile-cycling'>Cycling/<label for='mapbox-directions-profile-cycling'>Xe đạp/g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-tran-directions": "yarn run wemap-tran-directions-trafic && yarn run wemap-tran-directions-driving && yarn run wemap-tran-directions-walking && yarn run wemap-tran-directions-cycling",
    "wemap-tran-directions-key": "sed -i \"s|'access_token=' + accessToken|'key=' + accessToken|g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-directions-add-option": "sed -i \"/options.push('key=' + accessToken);/i options.push('&boundary.country=VNM'); \" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-directions-geocode": "sed -i \"s|this.api + encodeURIComponent(q.trim()) + '.json?'|'https://apis.wemap.asia/geocode-1/autocomplete?text='+ q.trim(),|g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-tran-directions-access-token": "sed -i \"s/if (accessToken) options.push('access_token=' + accessToken)/if (accessToken) options.push('key=' + accessToken)/g\" src/mapbox-gl-directions/dist/mapbox-gl-directions.js",
    "wemap-clean": "find dist/css/ -type f ! -name 'wemap.min.css' -delete",
    "wemap-update-config": "sed -i \"s/name: 'mapboxgl'/name: 'wemapgl'/g\" rollup.config.js",
    "wemap-gulp": "gulp build && cp src/wemap/css/style.min.css dist/css/dev.min.css",
    "wemap-copy-js": "mkdir -p dist/js && rm -rf dist/js/* && cp dist/mapbox-gl.js dist/js/wemap-gl.js",
    "wemap-dev-style": "cp dist/mapbox-gl.css dist/css && cp src/mapbox-gl-directions/src/mapbox-gl-directions.css dist/css && cp src/pelias-geocoder/pelias-geocoder.css dist/css",
    "wemap-dev": "yarn run build-prod-min && yarn run build-css && gulp build && mkdir -p dist/css && rm -rf dist/css/* && yarn run wemap-font-awesome && yarn run wemap-dev-style && gulp wemap-prod && yarn run wemap-clean && cp src/wemap/css/style.min.css dist/css/dev.min.css && yarn run wemap-copy-js",
    "wemap-prod-style": "cp dist/mapbox-gl.css dist/css && cp src/mapbox-gl-directions/src/mapbox-gl-directions.css dist/css && cp src/pelias-geocoder/pelias-geocoder.css dist/css && cp src/wemap/css/style.min.css dist/css",
    "wemap-prod": "yarn run build-prod-min && yarn run build-css && gulp build && mkdir -p dist/css && rm -rf dist/css/* && yarn run wemap-font-awesome && yarn run wemap-prod-style && gulp wemap-prod && yarn run wemap-clean && yarn run wemap-copy-js",
    "build-csp": "rollup -c rollup.config.csp.js",
    "build-query-suite": "rollup -c test/integration/rollup.config.test.js",
    "build-flow-types": "cp build/mapbox-gl.js.flow dist/mapbox-gl.js.flow && cp build/mapbox-gl.js.flow dist/mapbox-gl-dev.js.flow",
    "build-css": "postcss -o dist/mapbox-gl.css src/css/mapbox-gl.css",
    "build-style-spec": "cd src/style-spec && npm run build && cd ../.. && mkdir -p dist/style-spec && cp src/style-spec/dist/* dist/style-spec",
    "watch-css": "postcss --watch -o dist/mapbox-gl.css src/css/mapbox-gl.css",
    "build-token": "node build/generate-access-token-script.js",
    "build-benchmarks": "BENCHMARK_VERSION=${BENCHMARK_VERSION:-\"$(git rev-parse --abbrev-ref HEAD) $(git rev-parse --short=7 HEAD)\"} rollup -c bench/versions/rollup_config_benchmarks.js",
    "watch-benchmarks": "BENCHMARK_VERSION=${BENCHMARK_VERSION:-\"$(git rev-parse --abbrev-ref HEAD) $(git rev-parse --short=7 HEAD)\"} rollup -c bench/rollup_config_benchmarks.js -w",
    "start-server": "st --no-cache -H 0.0.0.0 --port 9966 --index index.html .",
    "start": "run-p build-token watch-css watch-query watch-benchmarks start-server",
    "start-debug": "run-p build-token watch-css watch-dev start-server",
    "start-tests": "run-p build-token watch-css watch-query start-server",
    "start-bench": "run-p build-token watch-benchmarks start-server",
    "start-release": "run-s build-token build-prod-min build-css print-release-url start-server",
    "lint": "eslint --cache --ignore-path .gitignore src test bench debug/*.html",
    "lint-docs": "documentation lint src/index.js",
    "lint-css": "stylelint 'src/css/mapbox-gl.css'",
    "test": "run-s lint lint-css lint-docs test-flow test-unit",
    "test-suite": "run-s test-render test-query test-expressions",
    "test-suite-clean": "find test/integration/{render,query, expressions}-tests -mindepth 2 -type d -exec test -e \"{}/actual.png\" \\; -not \\( -exec test -e \"{}/style.json\" \\; \\) -print | xargs -t rm -r",
    "test-unit": "build/run-tap --reporter classic --no-coverage test/unit",
    "test-build": "build/run-tap --no-coverage test/build/**/*.test.js",
    "test-render": "node --max-old-space-size=2048 test/render.test.js",
    "test-query-node": "node test/query.test.js",
    "watch-query": "testem -f test/integration/testem.js",
    "test-query": "testem ci -f test/integration/testem.js -R xunit > test/integration/query-tests/test-results.xml",
    "test-expressions": "build/run-node test/expression.test.js",
    "test-flow": "build/run-node build/generate-flow-typed-style-spec && flow .",
    "test-cov": "nyc --require=@mapbox/flow-remove-types/register --reporter=text-summary --reporter=lcov --cache run-s test-unit test-expressions test-query test-render",
    "prepublishOnly": "run-s build-flow-types build-dev build-prod-min build-prod build-csp build-css build-style-spec test-build",
    "prepublishOnlyWithoutTest": "run-s build-flow-types build-dev build-prod-min build-prod build-csp build-css build-style-spec",
    "print-release-url": "node build/print-release-url.js",
    "codegen": "build/run-node build/generate-style-code.js && build/run-node build/generate-struct-arrays.js"
  },
  "files": [
    "build/",
    "dist/",
    "flow-typed/",
    "src/",
    ".flowconfig"
  ]
}
